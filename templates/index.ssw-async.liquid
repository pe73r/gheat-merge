This file has been modified by the Yoast SEO for Shopify app.
You can learn more about this (and learn how to cleanly revert these changes) at https://yoa.st/removing-theme-changes.
{%- endcomment -%}
{%- liquid
	assign disabled_by_yoast_seo = true
	if content_for_header contains 'yoast-active-script'
		assign disabled_by_yoast_seo = false
	endif
-%}
{% layout none %}
{% assign customer_locale = shop.locale | prepend: 'lang_' %}
{% unless shop.metafields.ssw[customer_locale] %}
    {% for published_locale in shop.published_locales %}
        {% if published_locale.primary == true %}
            {% assign customer_locale = published_locale.iso_code | prepend: 'lang_' %}
        {% endif %}
    {% endfor %}
{% endunless %}
<!DOCTYPE html><html><head>{% render 'yoast-seo' %}<meta charset="UTF-8">{%- comment -%}Auto-disabled. Yoast plugin is taking care of Meta Tags output.{%- endcomment -%}
{%- if disabled_by_yoast_seo -%}
<meta name="robots" content="noindex">
{%- comment -%} End Yoast auto-disable. {%- endcomment -%}
{%- endif -%}<meta name="googlebot" content="noindex"><title></title></head><body><div id="ssw-async-html"><div id="ssw-discount-modal" class="ssw-modal ssw-fade" aria-hidden="true" role="modal"><div class="ssw-modal-dialog"><div class="ssw-modal-content"><div class="ssw-modal-body"><button type="button" class="ssw-close" data-dismiss="ssw-modal" aria-hidden="true">&times;</button><p id="discount-instructions">{{ shop.metafields.ssw[customer_locale].value.your_discount_code }}</p><div class="h1" id="ssw-discount-code"></div><br/><span id="discount-date"></span></div></div></div></div> {% capture ssw_helpers %}{% render 'ssw-helpers' %}{% endcapture %}
  {% unless ssw_helpers contains 'Liquid error' %}
    {{ ssw_helpers }}
  {% endunless %}
  {% capture socialshopwave_gplus %}{% render 'socialshopwave-gplus' %}{% endcapture %}
  {% unless socialshopwave_gplus contains 'Liquid error' %}
        {{ socialshopwave_gplus }}
  {% endunless %}</div></body></html>